{
  "name": "Log",
  "description": "Computes the logarithm value of your data.",
  "tags": [
    "preprocessing",
    "transformation"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/log_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "base",
      "defaultValue": 10,
      "description": "The base with respect to which logarithms are computed."
    },
    {
      "kind": "BooleanProperty",
      "name": "do.log1p",
      "defaultValue": false,
      "description": "Whether to add 1 to all values before computing log."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Values",
                "type": "numeric",
                "description": "Values to be logged.",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}